{
  "presets": [
    "react",
    [
      "latest",
      {
        "es2015": {
          "modules": false
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": [
          "Array"
        ]
      }
    ],
    ["styled-components", {
      "displayName": true
    }]
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        "latest"
      ]
    }
  }
}
